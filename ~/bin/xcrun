#!/bin/bash

# Custom xcrun wrapper to fix Swift version parsing issue
# This provides the expected format for Flutter builds

if [ "$1" == "version" ]; then
    # Flutter expects xcrun version to include Swift version info
    echo "xcrun version 70."
    echo "Apple Swift version 6.1.2 (swiftlang-6.1.2.1.2 clang-1700.0.13.5)"
else
    # For all other commands, pass through to real xcrun
    exec /usr/bin/xcrun "$@"
fi 